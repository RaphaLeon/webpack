{
  "name": "webpack-course",
  "version": "1.0.0",
  "description": "This is a great course!",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js --watch",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js --watch",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js --watch",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js --watch",
    "build:json": "webpack --config ./json-loader/webpack.config.js --watch",
    "build:react": "webpack --config ./react/webpack.config.js --watch",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js --watch",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js --watch"
  },
  "keywords": [
    "webpack"
  ],
  "author": "RaphaLeon",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.0.0",
    "json-loader": "0.5.7",
    "nib": "^1.1.2",
    "node-sass": "^4.5.3",
    "rupture": "^0.7.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
